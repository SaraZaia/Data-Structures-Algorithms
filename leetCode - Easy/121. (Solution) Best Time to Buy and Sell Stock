class Solution {
    public int maxProfit(int[] prices) {
        	
        if (prices.length == 0) return 0;
            
        int maxProfit = 0;
    	int min = prices[0];
    	
    	for (int i = 1; i < prices.length; i++) {
    		if (prices[i] < min ) {
                min = prices[i];
            }
            if (maxProfit < prices[i] - min )
            {
                maxProfit = prices[i] - min;
            }
		}
    	return maxProfit;
    }
}
